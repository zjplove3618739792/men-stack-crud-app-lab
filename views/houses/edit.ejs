<body>
    <h1>Edit <%= house.name %></h1>
    <form action="/houses/<%=house._id%>?_method=PUT" method="POST">
      <label for="name">Name:</label>
      <input type="text" name="name" id="name" />
  
      <label for="isReadyToBook">Ready to Book?</label>
      <input type="checkbox" name="isReadyToBook" <% if (house.isReadyToBook) { %>checked<% } %> >
      <button type="submit">Update House</button>
    </form>
    <a href="/houses">Back to Houses</a>
  </body>
  